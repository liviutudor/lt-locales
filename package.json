{
  "name": "lt-locales",
  "version": "1.0.0",
  "description": "Liviu Tudor's locale package. This package was written to allow locales such as `zh-Hant-TW` -- i.e. include the script in the locale string.",
  "main": "index.js",
  "entry point": "index.js",
  "scripts": {
    "test": "npm run test:unit",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "index.js"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "html",
      "text-summary"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "jest": "^29.3"
  },
  "author": "~liviutudor",
  "license": "Apache-2.0",
  "homepage": "https://github.com/liviutudor/lt-locales",
  "keywords": ["locale", "language", "country"],
  "repository": {
    "type": "https",
    "url": "https://github.com/liviutudor/lt-locales.git"
  }
}
