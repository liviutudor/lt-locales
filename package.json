    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "index.js"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "html",
      "text-summary"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "jest": "^22.4"
  },
